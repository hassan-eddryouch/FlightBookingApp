<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF"/>
        </shape>
    </item>

    <!-- Barcode Lines - Simulated Pattern -->
    <item android:left="10dp" android:right="10dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="2dp"
                android:color="#000000"
                android:dashWidth="3dp"
                android:dashGap="2dp"/>
        </shape>
    </item>

    <item android:left="20dp" android:right="20dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#000000"
                android:dashWidth="2dp"
                android:dashGap="3dp"/>
        </shape>
    </item>

    <item android:left="30dp" android:right="30dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="3dp"
                android:color="#000000"
                android:dashWidth="4dp"
                android:dashGap="2dp"/>
        </shape>
    </item>

    <item android:left="40dp" android:right="40dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="2dp"
                android:color="#000000"
                android:dashWidth="2dp"
                android:dashGap="4dp"/>
        </shape>
    </item>

    <item android:left="50dp" android:right="50dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#000000"
                android:dashWidth="3dp"
                android:dashGap="2dp"/>
        </shape>
    </item>

</layer-list>